{
  "version": 2,
  "buildCommand": "cd server && npm install",
  "installCommand": "cd server && npm install",
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "server/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/index.js"
    }
  ]
}
